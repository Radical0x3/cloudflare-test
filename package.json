{
  "name": "lynq-site",
  "version": "0.0.1",
  "packageManager": "pnpm@8.6.0",
  "private": true,
  "scripts": {
    "dev": "pnpm run gen:all && next dev",
    "dev:clear-cache": "pnpm run clear:cache && pnpm run clear:next && pnpm run dev",
    "dev:only": "next dev",
    "build": "next build",
    "build:clear-cache": "pnpm run clear:next && pnpm run build",
    "pages:build": "npx @cloudflare/next-on-pages",
    "pages:deploy": "pnpm run pages:build && wrangler pages deploy .vercel/output/static",
    "start": "next start",
    "pr-ready": "pnpm run check-ts && pnpm run gen:all && pnpm run analyze-code && pnpm run prettier:autofix && pnpm run eslint:all-autofix",
    "pr-ready:full": "pnpm run check-ts && pnpm run clear:cache && pnpm run gen:all && pnpm run analyze-code && pnpm run prettier:autofix && pnpm run eslint:all-autofix && pnpm run jest:coverage",
    "-------------------------------- TEMPLATES": "SECTION",
    "tpl": "node \"./libs/templates\"",
    "-------------------------------- GENERATE": "SECTION",
    "gen:all": "pnpm run gen:env && pnpm run gen:routes && pnpm run gen:graphql-all && pnpm run gen:translations",
    "gen:env": "node -e \"require('./libs/environment/generate-d-ts').generate('.env', './src/@types/process.env.d.ts')\" && prettier \"./src/@types/process.env.d.ts\" --write",
    "gen:routes": "node -e \"require('./libs/routes').generate()\" && prettier \"./src/Services/Routes/index-all-generated.ts\" --write",
    "gen:graphql-all": "pnpm run gen:graphql-introspect && pnpm run gen:graphql-codegen",
    "gen:graphql-introspect": "graphql-codegen --config \"./src/Services/GQL/codegen/introspect.ts\"",
    "gen:graphql-codegen": "graphql-codegen --config \"./src/Services/GQL/codegen/generate.ts\" && prettier \"./src/Services/GQL/generated/**\" --write",
    "gen:translations": "node -e \"require('./libs/translations').fetch()\" && prettier \"./src/Services/I18n/generated/**\" --write",
    "-------------------------------- TEST": "SECTION",
    "analyze-code": "node ./libs/analyze-code",
    "prettier": "prettier \"./{docs,libs,src}/**/*.{ts,tsx,js,jsx,json,graphql,md}\" --check",
    "prettier:autofix": "prettier \"./{docs,libs,src}/**/*.{ts,tsx,js,jsx,json,graphql,md}\" --check --write",
    "eslint:root": "eslint \"./*.js\" --cache --cache-location ./.cache/eslint/root-files",
    "eslint:project": "eslint \"./{libs,src}/**/**.{js,ts,tsx}\" --cache --cache-location ./.cache/eslint/project-files",
    "eslint:all-autofix": "eslint \"./*.js\" --cache --cache-location ./.cache/eslint/root-files --fix && eslint \"./{libs,src}/**/**.{js,ts,tsx}\" --cache --cache-location ./.cache/eslint/project-files --fix",
    "jest": "jest",
    "jest:coverage": "jest --coverage --collectCoverageFrom=\"./{libs,src}/**/*.ts\" && istanbul-badges-readme",
    "test-ci": "pnpm run analyze-code && pnpm run prettier && pnpm run eslint:root && pnpm run eslint:project && jest --verbose --runInBand",
    "test-ci:clear-cache": "pnpm run clear:cache && pnpm run test-ci",
    "-------------------------------- UTILS": "SECTION",
    "clear:cache": "del-cli ./.cache/",
    "clear:next": "del-cli ./.next/",
    "-------------------------------- CHECK": "SECTION",
    "check-ts": "tsc --noEmit"
  },
  "dependencies": {
    "@apollo/client": "^3.7.9",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/base": "^5.0.0-beta.12",
    "@mui/icons-material": "^5.14.6",
    "@mui/material": "^5.14.6",
    "@mui/system": "^5.14.6",
    "@mui/utils": "^5.14.6",
    "@mui/x-date-pickers": "^6.11.2",
    "@next/env": "^13.4.19",
    "@types/react-date-range": "1.4.3",
    "@wezom/toolkit-array-cjs": "^5.7.0",
    "@wezom/toolkit-css-in-js-cjs": "^5.7.0",
    "@wezom/toolkit-math-cjs": "^5.6.0",
    "@wezom/toolkit-promise-cjs": "^5.6.0",
    "apollo-upload-client": "^17.0.0",
    "camel-case": "^4.1.2",
    "chalk": "^4.1.2",
    "clsx": "^1.2.1",
    "constant-case": "^3.0.4",
    "cookie": "^0.5.0",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^2.0.0",
    "del": "^6.0.0",
    "del-cli": "^4.0.1",
    "from-cwd": "^1.0.1",
    "graphql": "^16.8.0",
    "graphql-tag": "^2.12.6",
    "inquirer": "^8.2.0",
    "isomorphic-unfetch": "^3.1.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "lodash.isempty": "^4.4.0",
    "lodash.set": "^4.3.2",
    "lodash.throttle": "^4.1.1",
    "mkdirp": "^1.0.4",
    "next": "14.0.0",
    "nookies": "^2.5.2",
    "notiflix": "^3.2.6",
    "nprogress": "^0.2.0",
    "param-case": "^3.0.4",
    "pascal-case": "^3.1.2",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-date-range": "1.4.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^12.0.5",
    "react-hook-form": "^7.19.5",
    "react-intersection-observer": "8.33.1",
    "react-phone-input-2": "^2.15.1",
    "react-window": "^1.8.9",
    "short-hash": "^1.0.0",
    "socket.io-client": "^4.7.2",
    "stylis": "^4.0.13",
    "subscriptions-transport-ws": "^0.11.0",
    "swiper": "^11.0.5"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@cloudflare/next-on-pages": "^1.7.2",
    "@graphql-codegen/cli": "^5.0.0",
    "@mui/types": "^7.2.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/apollo-upload-client": "^17.0.2",
    "@types/cookie": "^0.4.1",
    "@types/jest": "^29.5.4",
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.isempty": "^4.4.7",
    "@types/lodash.set": "^4.3.6",
    "@types/lodash.throttle": "^4.1.6",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.2.21",
    "@types/react-beautiful-dnd": "^13.1.4",
    "@types/react-dom": "^18.2.7",
    "@types/react-input-mask": "^3.0.2",
    "@types/react-window": "^1.8.5",
    "@types/request-ip": "^0.0.37",
    "@types/stylis": "^4.0.2",
    "@wezom/eslint-config": "^11.0.5",
    "@wezom/eslint-config-ts": "^11.0.5",
    "@wezom/eslint-config-ts-react": "^11.0.5",
    "@wezom/graphql-codegen-configurator": "^0.1.1",
    "@wezom/toolkit-jest": "^3.4.1",
    "duplexer": "^0.1.2",
    "eslint": "^8.47.0",
    "eslint-config-next": "14.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.2.3",
    "fork-ts-checker-webpack-plugin": "^6.4.0",
    "glob": "^7.2.0",
    "highlight.js": "^11.3.1",
    "istanbul-badges-readme": "^1.6.0",
    "jest": "^27.3.1",
    "markdown-pdf": "^10.0.0",
    "prettier": "^2.5.1",
    "remarkable-classy": "^0.0.1",
    "split": "^1.0.1",
    "through": "^2.3.8",
    "ts-jest": "^27.0.7",
    "typescript": "^5.0.4",
    "vercel": "^32.7.0",
    "yargs": "^17.4.1"
  }
}
